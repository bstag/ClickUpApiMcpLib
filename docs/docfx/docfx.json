{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "../../src/ClickUp.Api.Client.Abstractions/ClickUp.Api.Client.Abstractions.csproj",
            "../../src/ClickUp.Api.Client.Models/ClickUp.Api.Client.Models.csproj",
            "../../src/ClickUp.Api.Client/ClickUp.Api.Client.csproj"
          ],
          "src": "../../"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties": {
        "TargetFramework": "net9.0"
      },
      "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "styles/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appTitle": "ClickUp .NET SDK Documentation",
      "_appName": "ClickUp.Api.Client",
      "_appLogoPath": "images/logo.png",
      "_appFaviconPath": "images/favicon.ico",
      "_appFooter": "© 2024 ClickUp .NET SDK Contributors. Licensed under MIT License.",
      "_gitContribute": {
        "repo": "https://github.com/your-org/ClickUpApiMcpLib",
        "branch": "main"
      },
      "_gitUrlPattern": "github",
      "_enableSearch": true,
      "_enableNewTab": true,
      "disableGitFeatures": false,
      "_disableContribution": false,
      "_disableBreadcrumb": false,
      "_disableAffix": false,
      "_disableNextArticle": false,
      "_lang": "en-us"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "postProcessors": ["ExtractSearchIndex"],
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}